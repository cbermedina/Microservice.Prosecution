{
  "ConnectionStrings": {
    "DefaultConnection": "mongodb://localhost"
  },
  "MongoConnection": {
    "ConnectionString": "mongodb://localhost:27017/?readPreference=primary&appname=MongoDB%20Compass&ssl=false",
    "Database": "HangFireProcess"
  },
  "GeneralConfiguration": {
    "ServerName": "Process", //Nombre Servidor
    "WorkersNumber": 30, //numero Trabajadores
    "TimeToQueue": 60, //tiempo Encolamiento
    "ProcessRetriesNumber": 5
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Polly": {
    "MaxTrys": 3,
    "TimeDelay": 5
  },
  "ServiceUrl": {
    "Url": ""
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "Cache": {
    "CacheExpireInMinutes": 30
  },
  "ProcessMongoDB": {
    "HangfireName": "HangfireProcess"
  }
}
